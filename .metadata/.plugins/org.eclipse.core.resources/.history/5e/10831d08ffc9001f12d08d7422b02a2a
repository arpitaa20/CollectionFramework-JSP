package jsp.Collection;

public class Driver {

	public static void main(String[] args) {
		Inheritance father = new Inheritance(50000.00, "GT", "BMW", "Activa");
		father.fatherDetails();
		
		Son son=new Son();
		System.out.println("Son's property-------");
		son.displayDetails();
		
		Daughter daughter = new Daughter();
		System.out.println("Daughter's property----");
		daughter.daughterDetails();
		
	}

}
